{
  "version": 8,
  "name": "Mon Style Perso (Contours dynamiques)",
  "metadata": {},
  "sources": {
    "mymaptiles": {
      "type": "vector",
      "tiles": [
        "https://tiles.dbark.io/{z}/{x}/{y}.pbf"
      ],
      "maxzoom": 14
    }
  },
  "glyphs": "https://orangemug.github.io/font-glyphs/glyphs/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "bg",
      "type": "background",
      "paint": {
        "background-color": "rgba(228, 230, 236, 1)"
      }
    },
    {
      "id": "water",
      "type": "fill",
      "source": "mymaptiles",
      "source-layer": "water",
      "paint": {
        "fill-color": "rgba(124, 211, 255, 1)"
      }
    },
    {
      "id": "boundary",
      "type": "line",
      "source": "mymaptiles",
      "source-layer": "boundary",
      "paint": {
        "line-color": "rgba(213, 205, 205, 1)"
      }
    },
    {
      "id": "building",
      "type": "fill",
      "source": "mymaptiles",
      "source-layer": "building",
      "layout": {
        "visibility": "none"
      },
      "paint": {
        "fill-color": "rgba(205, 204, 204, 1)",
        "fill-outline-color": "rgba(143, 58, 58, 1)"
      }
    },
    {
      "id": "landcover-parks",
      "type": "fill",
      "source": "mymaptiles",
      "source-layer": "landcover",
      "filter": [
        "==",
        "class",
        "grass"
      ],
      "paint": {
        "fill-color": "#b6e3a1",
        "fill-opacity": 0.5
      }
    },
    {
      "id": "rues-locales-contour",
      "type": "line",
      "source": "mymaptiles",
      "source-layer": "transportation",
      "minzoom": 13,
      "filter": [
        "all",
        [
          "==",
          "$type",
          "LineString"
        ],
        [
          "in",
          "class",
          "minor",
          "street",
          "residential",
          "service"
        ]
      ],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "rgba(216, 214, 214, 1)",
        "line-width": [
          "interpolate",
          [
            "linear"
          ],
          [
            "zoom"
          ],
          13,
          2,
          16,
          12,
          20,
          28
        ]
      }
    },
    {
      "id": "rues-locales-remplissage",
      "type": "line",
      "source": "mymaptiles",
      "source-layer": "transportation",
      "minzoom": 13,
      "filter": [
        "all",
        [
          "==",
          "$type",
          "LineString"
        ],
        [
          "in",
          "class",
          "minor",
          "street",
          "residential",
          "service"
        ]
      ],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "rgba(250, 250, 250, 1)",
        "line-width": [
          "interpolate",
          [
            "linear"
          ],
          [
            "zoom"
          ],
          13,
          1,
          16,
          10,
          20,
          24
        ]
      }
    },
    {
      "id": "routes-principales-contour",
      "type": "line",
      "source": "mymaptiles",
      "source-layer": "transportation",
      "minzoom": 5,
      "filter": [
        "all",
        [
          "==",
          "$type",
          "LineString"
        ],
        [
          "in",
          "class",
          "motorway",
          "trunk",
          "primary",
          "secondary",
          "tertiary"
        ]
      ],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "rgba(216, 214, 214, 1)",
        "line-opacity": [
          "interpolate",
          [
            "linear"
          ],
          [
            "zoom"
          ],
          12,
          1,
          13,
          0
        ],
        "line-width": [
          "interpolate",
          [
            "linear"
          ],
          [
            "zoom"
          ],
          5,
          1.5,
          10,
          6,
          16,
          26
        ]
      }
    },
    {
      "id": "routes-principales",
      "type": "line",
      "source": "mymaptiles",
      "source-layer": "transportation",
      "minzoom": 5,
      "filter": [
        "all",
        [
          "==",
          "$type",
          "LineString"
        ],
        [
          "in",
          "class",
          "motorway",
          "trunk",
          "primary",
          "secondary",
          "tertiary"
        ]
      ],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "rgba(255, 255, 255, 1)",
        "line-width": [
          "interpolate",
          [
            "linear"
          ],
          [
            "zoom"
          ],
          5,
          0.5,
          10,
          4,
          16,
          22
        ]
      }
    },
    {
      "id": "noms-des-rues",
      "type": "symbol",
      "source": "mymaptiles",
      "source-layer": "transportation_name",
      "minzoom": 12,
      "layout": {
        "text-field": "{name}",
        "text-font": [
          "Open Sans Regular"
        ],
        "text-size": 12,
        "symbol-placement": "line",
        "text-max-angle": 30
      },
      "paint": {
        "text-color": "#333333",
        "text-halo-color": "#ffffff",
        "text-halo-width": 2
      }
    }
  ],
  "id": "hlhi32z"
}
